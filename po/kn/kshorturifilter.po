#: kshorturifilter.cpp:278
#, kde-format
msgid "<qt><b>%1</b> does not have a home folder.</qt>"
msgstr "<qt><b>%1</b> ನೆಲೆ ಕಡತಕೋಶವನ್ನು ಹೊಂದಿಲ್ಲ.</qt>"

#: kshorturifilter.cpp:280
#, kde-format
msgid "<qt>There is no user called <b>%1</b>.</qt>"
msgstr "<qt><b>%1</b> ಎಂಬ ಹೆಸರಿನ ಯಾವುದೆ ಬಳಕೆದಾರನಿಲ್ಲ.</qt>"

#: kshorturifilter.cpp:528
#, kde-format
msgid "<qt>The file or folder <b>%1</b> does not exist.</qt>"
msgstr "<qt><b>%1</b> ಎಂಬ ಕಡತಕೋಶವು ಅಸ್ತಿತ್ವದಲ್ಲಿಲ್ಲ.</qt>"
