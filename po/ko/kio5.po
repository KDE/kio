msgstr "<li>프로토콜: %1</li>"

#: core/job_error.cpp:302
#, kde-format
msgid "<li>Date and time: %1</li>"
msgstr "<li>날짜 및 시간: %1</li>"

#: core/job_error.cpp:302
#, kde-format
