msgstr "Ni mapa"

#: core/klocalsocket_unix.cpp:303
msgid "Read-only filesystem"
msgstr "Datotečni sistem samo za branje"

#: core/klocalsocket_unix.cpp:370 core/klocalsocket_unix.cpp:404
msgid "Unknown socket error"
