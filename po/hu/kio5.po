#, kde-format
msgid "(Symbolic Link to %1)"
msgstr "(Szimbolikus link erre: %1)"

#: core/kfileitem.cpp:1188
#, kde-format
msgid "(%1, Link to %2)"
msgstr "(%1, link erre: %2)"

#: core/kfileitem.cpp:1191
#, kde-format
msgid " (Points to %1)"
msgstr " (Ide mutat: %1)"

#: core/klocalsocket_unix.cpp:192 core/klocalsocket_unix.cpp:260
