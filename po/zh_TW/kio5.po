msgstr "<b>要求的細節</b>："

#: core/job_error.cpp:298
#, kde-format
msgid "<li>URL: %1</li>"
msgstr "<li>URL：%1</li>"

#: core/job_error.cpp:300
#, kde-format
msgid "<li>Protocol: %1</li>"
msgstr "<li>協定：%1</li>"

#: core/job_error.cpp:302
#, kde-format
