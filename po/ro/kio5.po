#, kde-format
msgid "<b>Technical reason</b>: "
msgstr "<b>Motiv tehnic</b>: "

#: core/job_error.cpp:298
#, kde-format
msgid "<b>Details of the request</b>:"
msgstr "<b>Detaliile cererii</b>:"

#: core/job_error.cpp:298
#, kde-format
msgid "<li>URL: %1</li>"
msgstr "<li>URL: %1</li>"

#: core/job_error.cpp:300
#, kde-format
msgid "<li>Protocol: %1</li>"
msgstr "<li>Protocol: %1</li>"

#: core/job_error.cpp:302
#, kde-format
msgid "<li>Date and time: %1</li>"
msgstr "<li>Data și ora: %1</li>"

#: core/job_error.cpp:302
#, kde-format
msgid "<li>Additional information: %1</li>"
msgstr "<li>Informații adiționale: %1</li>"

#: core/job_error.cpp:304
#, kde-format
msgid "<b>Possible causes</b>:"
msgstr "<b>Cauze posibile</b>:"

#: core/job_error.cpp:308
#, kde-format
