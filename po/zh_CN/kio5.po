msgid "<li>Date and time: %1</li>"
msgstr "<li>日期和时间：%1</li>"

#: core/job_error.cpp:307
#, kde-format
msgid "<li>Additional information: %1</li>"
msgstr "<li>额外信息：%1</li>"

#: core/job_error.cpp:309
#, kde-format
